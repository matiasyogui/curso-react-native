{
  "expo": {
    "name": "reddit-clone",
    "slug": "reddit-clone",
    "version": "1.0.0",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "icon": "./assets/reddit-icon.png",
    "splash": {
      "image": "./assets/reddit-icon-splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.technisys.redditclone",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.technisys.redditclone",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/reddit-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
